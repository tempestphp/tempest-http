{
    "name": "tempest/http",
    "description": "Standard HTTP request/response objects.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.5",
        "tempest/core": "3.0.0",
        "tempest/clock": "3.0.0",
        "tempest/console": "3.0.0",
        "tempest/mapper": "3.0.0",
        "tempest/container": "3.0.0",
        "tempest/cryptography": "3.0.0",
        "laminas/laminas-diactoros": "^3.3",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0|^2.0",
        "symfony/uid": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Tempest\\Http\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Http\\Tests\\": "tests"
        }
    }
}
